        <activity
            android:name=".activity.MyYinHangKa"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.MyTuiGuangAcitivty"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.SettingActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.AlreadAuthAcitity"
